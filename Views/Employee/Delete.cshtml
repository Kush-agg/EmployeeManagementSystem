@model Employee
@Html.AntiForgeryToken()
<form method="post" asp-action="Delete">
  <input asp-for ="employeeId" hidden />
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Delete Employee <hr /></h2>
            
        </div>
         <div class="mb-3">
        <label asp-for="firstName"></label>
        <input asp-for="firstName" disabled class="form-control" />
        
       </div>
      <div class="mb-3">
        <label asp-for="lastName"></label>
        <input asp-for="lastName" disabled class="form-control" />
        
      </div>
       <div class="mb-3">
        <label asp-for="dateOfJoining"></label>
        <input asp-for="dateOfJoining" disabled class="form-control" />
        
       </div>
       <div class="mb-3">
        <label asp-for="designation"></label>
        <input asp-for="designation" disabled class="form-control" />
        
       </div>
       <div class="mb-3">
        <label asp-for="email"></label>
        <input asp-for="email" disabled class="form-control" />
        
       </div>
      <button type="submit" class="btn btn-danger" style="width: 150px;">Delete </button>
      <a asp-asp-controller="Employee" asp-action="Index" class="btn btn-secondary" style="width: 150px;">
        Back to List
      </a>
    </div>

</form>

@section Scripts{
    @{
      <partial name="_ValidationScriptsPartial" />
    }
}